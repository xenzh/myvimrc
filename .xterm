! xoria256 theme (from https://github.com/derekwyatt/dotfiles/blob/master/rxvt/themes/xoria256.rxvt)
XTerm*color0         : #444444
XTerm*color1         : #d78787
XTerm*color2         : #afd787
XTerm*color3         : #f7f7af
XTerm*color4         : #87afd7
XTerm*color5         : #d7afd7
XTerm*color6         : #87afd7
XTerm*color7         : #e6e6e6
XTerm*color8         : #0a0a0a
XTerm*color9         : #df8787
XTerm*color10        : #afdf87
XTerm*color11        : #ffffaf
XTerm*color12        : #87afdf
XTerm*color13        : #dfafdf
XTerm*color14        : #87afd7
XTerm*color15        : #eeeeee
XTerm*colorBD        : #afafdf
XTerm*colorIT        : _colorIT
XTerm*colorUL        : #dfdfaf
XTerm*foreground     : #d0d0d0
XTerm*background     : #1c1c1c
XTerm*cursorColor    : #ffaf00

xterm*colorBDMode: true
xterm*colorBD: #e5e5e5


xterm*termName: xterm-256color
xterm*appkeypadDefault: false
xterm*locale: true
xterm*utf8: 1


xterm*renderFont: true
xterm*faceName: DejaVu Sans Mono:style=Regular
xterm*faceSize: 10
xterm*boldFont: *-mono-*-*-*-12-*
xterm*boldMode: false


XTerm.vt100.saveLines: 4096
XTerm.vt100.scrollBar: true
XTerm.vt100.scrollbar.width: 10
XTerm.vt100.geometry: 120x40
XTerm.vt100.metaSendsEscape: true
